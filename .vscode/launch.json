    "version": "0.2.0",
    "configurations": [
        {
            "name": "FinnHub Prod",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=envFile=.env"
            ]
        },
        {
            "name": "FinnHub Dev",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=envFile=.env.dev"
            ]
        },
        {
            "name": "finnhub_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "finnhub_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}